exclude_paths:
  - .github/
  - roles/docker
  - roles/firewalld
  - roles/postgresql
  - roles/ssl_certificates
  - roles/install_java
  - roles/install_python
  - roles/tomcat

warn_list:
  - command-instead-of-module
  - no-handler
  - package-latest

var_naming_pattern: "^[a-z_][a-z0-9_]*$"

skip_list:
  - var-naming
